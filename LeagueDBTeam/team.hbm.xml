<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="Team" table="TEAM">

      <id name="teamID" column="TEAMID" not-null="true">
	  	<generator class="native"/>
	  </id>

	  <property name="league" not-null="true">
		 <column name="LEAGUE" />
	  </property>

	  <property name="sponsor" not-null="false">
		 <column name="SPONSOR" />
	  </property>
	  
	  <property name="teamName" not-null="true">
		 <column name="teamName" />
	  </property>
	  
	  <property name="headCoach" not-null="false">
		 <column name="HEADCOACH" />
	  </property>
	  
	  <property name="asstCoach" not-null="false">
		 <column name="ASSTCOACH" />
	  </property>
	  
	  <property name="trainer" not-null="false">
		 <column name="TRAINER" />
	  </property>
	  
	  <property name="manager" not-null="false">
		 <column name="MANAGER" />
	  </property>
	  
	  <property name="website" not-null="false">
		 <column name="WEBSITE" />
	  </property>
	  
	  <many-to-one name="league" class="League"
	  	<column name="LEAGUE"/>
	  </many-to-one>
	  
	  <many-to-one name="headCoach" class="Staff"
	  	<column name="HEADCOACH"/>
	  </many-to-one>
	  
	  <many-to-one name="asstCoach" class="Staff"
	  	<column name="ASSTCOACH"/>
	  </many-to-one>
	  
	  <many-to-one name="trainer" class="Staff"
	  	<column name="TRAINER"/>
	  </many-to-one>
	  
	  <many-to-one name="manager" class="Staff"
	  	<column name="MANAGER"/>
	  </many-to-one>
	  
   </class>

</hibernate-mapping>