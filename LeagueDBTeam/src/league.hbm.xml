<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="IO.League" table="LEAGUE">

      <id name="leagueID">
	  	<column name="LEAGUEID" not-null="true"/>
	  	<generator class="native"/>
	  </id>

	  <property name="leagueName" >
		 <column name="LEAGUENAME" not-null="true"/>
	  </property>

	  <property name="leagueSponsor">
		 <column name="LEAGUESPONSOR" />
	  </property>
   </class>

</hibernate-mapping>